{
   "name": "Python Codespace",
   "image": "mcr.microsoft.com/devcontainers/python:3.10", // Specify Python version
   "customizations": {
     "vscode": {
       "extensions": [
         "ms-python.python",
         "ms-toolsai.jupyter"
       ]
     }
   },
   "postCreateCommand": "pip install -r requirements.txt",
   "portsAttributes": {
      "8000": {
         "label": "Jupyter Server",
         "onAutoForward": "notify"
      }
   }
}
